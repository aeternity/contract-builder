{
  "name": "@aeternity/contract-builder",
  "version": "0.1.0",
  "description": "Sophia smart contracts builder",
  "main": "dist/loader.mjs",
  "types": "dist/loader.d.ts",
  "exports": "./dist/loader.js",
  "bin": {
    "ae-contract-builder": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && babel src --out-dir dist --extensions .ts --source-maps true",
    "commitlint": "commitlint --from develop",
    "lint": "prettier . --check && eslint .",
    "format": "prettier . --write",
    "test": "mocha './test/'",
    "test:watch": "mocha './test/' --watch",
    "prepare": "npm run build",
    "prepublishOnly": "test/examples.sh",
    "release": "standard-version --skip.tag"
  },
  "license": "ISC",
  "keywords": [
    "aeternity",
    "blockchain",
    "Sophia",
    "smart contracts"
  ],
  "dependencies": {
    "@aeternity/aepp-sdk": "^13.3.3",
    "@types/lodash": "^4.17.7",
    "commander": "^12.1.0",
    "lodash": "^4.17.21",
    "schema-utils": "^4.2.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aeternity/contract-builder"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/register": "^7.24.6",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.10.0",
    "@types/chai": "^4.3.19",
    "@types/mocha": "^10.0.8",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "chai": "^4.3.7",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "memfs": "^4.12.0",
    "mocha": "^10.7.3",
    "nyc": "^17.1.0",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.5",
    "typescript-eslint": "^8.6.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "browserslist": [
    "node >= 14.19"
  ],
  "engines": {
    "node": ">=14.19.0"
  }
}
